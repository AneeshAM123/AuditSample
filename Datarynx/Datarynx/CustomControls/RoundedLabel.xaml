<?xml version="1.0" encoding="UTF-8"?>
<ContentView
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Datarynx.CustomControls.RoundedLabel">
    <ContentView.Content>
        <Frame
            x:Name="borderFrame"
            Grid.Column="3"
            HeightRequest="25"
            WidthRequest="25"
            CornerRadius="12.5"
            HasShadow="False"
            Padding="0"
            HorizontalOptions="Center"
            VerticalOptions="Center"
            BorderColor="Black">
            <Label
                x:Name="content"
                VerticalOptions="Center"
                FontSize="14"
                HorizontalOptions="Center"
                TextColor="Black"/>
       </Frame>
    </ContentView.Content>
</ContentView>
