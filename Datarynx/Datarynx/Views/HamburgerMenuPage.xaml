    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Datarynx.Views.HamburgerMenuPage"
    xmlns:views="clr-namespace:Datarynx.Views">
    <NavigationPage.HasNavigationBar>false</NavigationPage.HasNavigationBar>
    <MasterDetailPage.Master>
        <views:MenuPage />
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage>
           <x:Arguments>
                <views:AboutPage/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
    
</MasterDetailPage>
